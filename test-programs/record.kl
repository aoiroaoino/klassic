record Person {
  name: *
  age: Int
}

record Pair <a', b'> {
  _1: a'
  _2: b'
}

val point = new #Point(1000, 2000)
assert(1000 == point->x)
assert(2000 == point->y)

val person = new #Person("Kota Mizushima", 33)
println(person->name)
println(person->age)

val pair = new #Pair(1, 2)
assert(1 == pair->_1)
assert(2 == pair->_2)

